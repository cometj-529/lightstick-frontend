{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "SOCKET_URL": "https://socket.hxxstar.site",
        "API_URL": "https://api.hxxstar.site"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
